{
  "name": "tsumiqiita",
  "version": "1.8.1",
  "description": "MarkdownファイルをQiitaに投稿するelectron製デスクトップアプリです。",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/electron .",
    "build": "node_modules/.bin/build"
  },
  "build": {
    "productName": "TsumiQiita",
    "appId": "tsumiqiita",
    "mac": {
      "target": "dmg"
    },
    "directories": {
      "output": "../build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hidao80/TsumiQiita.git"
  },
  "author": "hidao80",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hidao80/TsumiQiita/issues"
  },
  "homepage": "https://github.com/hidao80/TsumiQiita#readme",
  "devDependencies": {
    "electron": "^2.0.12",
    "electron-builder": "^20.29.0"
  },
  "dependencies": {
    "escape-html": "^1.0.3",
    "github-markdown-css": "^2.10.0",
    "highlight.js": "^9.12.0",
    "markdown-it": "^8.4.1",
    "markdown-it-checkbox": "^1.1.0",
    "qiita-js": "^0.4.3"
  }
}
